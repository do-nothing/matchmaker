<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.microwise.smartservices.persistency.mapper.Power_controller_portMapper">
    <select id="updateStatusByPidAndIndex">
        update power_controller_ports set status = #{status} where pid = #{pid} and `index` = #{index};
    </select>
</mapper>
